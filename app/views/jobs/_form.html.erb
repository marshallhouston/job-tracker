<section class='form'>
  <%= form_for [@company, @job] do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %><br>

    <%= f.label :description %>
    <%= f.text_field :description %><br>

    <%= f.label :level_of_interest %>
    <%= f.number_field :level_of_interest %><br>

    <%= f.label :city %>
    <%= f.text_field :city %><br>

    <%= f.collection_select :category_id, @categories, :id, :title %><br>

    <%= f.submit %>
  <% end %>
</section>
